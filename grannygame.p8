pico-8 cartridge // http://www.pico-8.com
version 41
__lua__
--my first game
x=60
y=60
function _draw()
  cls(1)
  spr(1,x,y)
  if (btn(0)) x -= 2
  if (btn(1)) x += 2
  if (btn(2)) y -= 2
  if (btn(3)) y += 2
  if (btn(4)) circfill(x,y,5,5)
end
